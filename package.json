    "framer-motion": "11.0.5",
    "fs-extra": "^11.2.0",
    "isbot": "^4.4.0",
    "miniflare": "^3.20240925.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "rehype-img-size": "^1.0.1",
    "rehype-slug": "^6.0.0",
    "three": "^0.161.0",
    "three-stdlib": "^2.33.0"
  },
  "devDependencies": {
    "@csstools/postcss-global-data": "^2.1.1",
